    <DisableProjectBuild>true</DisableProjectBuild>
  </PropertyGroup>
  <ItemGroup />
  <PropertyGroup>
    <DebugType>Full</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="bleref.il" />
  </ItemGroup>
  <ItemGroup />
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.targets))\dir.targets" />
</Project>